# APPLICATION NAME (unique app-service-id)
APP_SID=easydock

# APP PORT
APP_PORT=80

# PHP VERSION 
PHP_V=8.0
# available values: 5.6, 7.1, 7.2, 7.3, 7.4 and 8.0

# MYSQL DB NAME
DB_NAME=dockerdb

# MYSQL ROOTPASSWORD
DB_PASS=secret

# MYSQL DB PORT
DB_PORT=3306

# REDIS PORT
RDS_PORT=6379

# PHPMYADMIN POST
PMA_PORT=8080

# MAILHOG PORT
MH_PORT=8081

# NGINX MODE
NGINX_MODE=public
# root - index.php "catch all" and ./ as Document Root (use it for Codigniter, WP, ...)
# public - index.php "catch all" and ./public as Document Root (use it for Laravel, Symfony, ...)
# build - index.html "catch all" and ./build as Document Root (use it for Vue, React, ...)

# APPLICATION SOURCE FOLDER
APP_SRC=./
